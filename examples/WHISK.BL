import whisk

build_examples(binary) = [minimal(binary), protobuf(binary), antlr(binary), submodules(binary)]

minimal(binary) = exec(work_dir='minimal', command='/usr/bin/java', arguments=['-jar', binary, 'build'])
protobuf(binary) = exec(work_dir='protobuf', command='/usr/bin/java', arguments=['-jar', binary, 'build'])
antlr(binary) = exec(work_dir='antlr', command='/usr/bin/java', arguments=['-jar', binary, 'build'])
submodules(binary) = exec(work_dir='submodules', command='/usr/bin/java', arguments=['-jar', binary, 'build'])